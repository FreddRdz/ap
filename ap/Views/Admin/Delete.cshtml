@model ap.Models.Administrador

<h2>Eliminar Administrador</h2>

<p>¿Estás seguro de que deseas eliminar este administrador?</p>

<p>
    <strong>Nombre:</strong> @Model.Nombre
</p>
<p>
    <strong>Correo:</strong> @Model.Correo
</p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a href="@Url.Action("Index")" class="btn btn-secondary">Cancelar</a>
}
